{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
    "indent": [2, 4],
    "linebreak-style": 0,
    "import/no-unresolved": 0,
    "no-unused-vars": 1,
    "no-shadow": 0,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "no-underscore-dangle": 0,
    "max-len": [1,
      {
        "code": 120,
        "ignoreComments": true
      }
    ],
    "no-param-reassign": 0
  }
}
